<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>personalKnowledgeMgr</display-name>
  
  <servlet>
  	<servlet-name>AjaxServlet</servlet-name>
  	<servlet-class>com.jacky.study.servlet.AjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AjaxServlet</servlet-name>
  	<url-pattern>/AjaxServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>JsonpServlet</servlet-name>
  	<servlet-class>com.jacky.study.servlet.JsonpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JsonpServlet</servlet-name>
  	<url-pattern>/JsonpServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>